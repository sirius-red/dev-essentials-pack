{
    "name": "dev-essentials-pack",
    "displayName": "Dev Essentials Pack",
    "description": "A collection of essential extensions for developers that I use in all my VSCode profiles.",
    "publisher": "SiriusRed",
    "version": "3.1.0",
    "icon": "assets/icon_128.png",
    "license": "MIT",
    "author": {
        "name": "Sirius Red",
        "url": "https://github.com/sirius-red"
    },
    "homepage": "https://github.com/sirius-red/dev-essentials-pack",
    "repository": {
        "type": "git",
        "url": "https://github.com/sirius-red/dev-essentials-pack"
    },
    "bugs": {
        "url": "https://github.com/sirius-red/dev-essentials-pack/issues"
    },
    "scripts": {
        "ext:setup": "pnpm install && vsce login SiriusRed",
        "ext:release": "node scripts/release.js",
        "ext:update": "node scripts/release.js --update-only",
        "ext:package": "vsce package",
        "ext:publish": "vsce publish",
        "fmt": "prettier --write **/*.{js,json,md}"
    },
    "type": "module",
    "engines": {
        "vscode": "^1.97.0"
    },
    "categories": [
        "Extension Packs"
    ],
    "extensionPack": [
        "adpyke.codesnap",
        "bierner.markdown-preview-github-styles",
        "christian-kohler.path-intellisense",
        "davidanson.vscode-markdownlint",
        "dotenv.dotenv-vscode",
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "exiasr.hadolint",
        "foxundermoon.shell-format",
        "github.codespaces",
        "harrydowning.yaml-embedded-languages",
        "humao.rest-client",
        "intellsmi.comment-translate",
        "ionutvmi.reg",
        "jeff-hykin.better-dockerfile-syntax",
        "jeff-hykin.better-shellscript-syntax",
        "mads-hartmann.bash-ide-vscode",
        "mamacarlos.envautoexample",
        "meronz.manpages",
        "mkhl.direnv",
        "monish.regexsnippets",
        "ms-azuretools.vscode-docker",
        "ms-cst-e.vscode-devskim",
        "ms-vscode-remote.remote-wsl",
        "ms-vscode.cmake-tools",
        "ms-vscode.makefile-tools",
        "ms-vscode.powershell",
        "mtxr.sqltools",
        "ndonfris.fish-lsp",
        "rechinformatica.rech-editor-batch",
        "redhat.vscode-xml",
        "redhat.vscode-yaml",
        "remisa.shellman",
        "rogalmic.bash-debug",
        "shinotatwu-ds.file-tree-generator",
        "streetsidesoftware.code-spell-checker",
        "streetsidesoftware.code-spell-checker-cspell-bundled-dictionaries",
        "tamasfe.even-better-toml",
        "tanhakabir.rest-book",
        "tetradresearch.vscode-h2o",
        "timonwong.shellcheck",
        "uvkush.all-code-one-file",
        "visualstudioexptteam.intellicode-api-usage-examples",
        "visualstudioexptteam.vscodeintellicode",
